{
    "name": "auth",
    "version": "0.0.11",
    "description": "",
    "author": "MaridiM Developments",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "npm": ">=9.0.0 <= 10.1.0",
        "node": ">=18.0.0 <= 20.9.0"
    },
    "scripts": {
        "prebuild": "rm -rf dist && npm cache clean --force",
        "build": "npm install && npx @nestjs/cli build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "cross-env NODE_ENV=production nest start",
        "start:dev": "cross-env NODE_ENV=development nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "cross-env NODE_ENV=production node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest.js --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/apollo": "^12.0.9",
        "@nestjs/common": "^10.2.6",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.2.6",
        "@nestjs/graphql": "^12.0.9",
        "@nestjs/platform-express": "^10.2.6",
        "@nestjs/typeorm": "^10.0.0",
        "@sendgrid/mail": "^7.7.0",
        "@types/jest": "^29.5.5",
        "apollo-server-express": "^3.10.1",
        "bcrypt": "^5.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cloudinary": "^1.41.0",
        "cookie-parser": "^1.4.6",
        "cross-env": "^7.0.3",
        "fs-capacitor": "^8.0.0",
        "graphql": "^16.8.1",
        "graphql-upload": "13.0.0",
        "i18next": "^23.5.1",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "jest": "^29.7.0",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^5.0.5",
        "rxjs": "^7.8.1",
        "twilio": "^4.18.0",
        "typeorm": "^0.3.17",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.1.18",
        "@nestjs/schematics": "^10.0.2",
        "@nestjs/testing": "^10.2.6",
        "@types/cookie-parser": "^1.4.4",
        "@types/express": "^4.17.18",
        "@types/multer": "^1.4.8",
        "@types/node": "^20.8.2",
        "@types/supertest": "^2.0.14",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.3",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "4.2.0",
        "typescript": "^5.2.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "verbose": true,
        "collectCoverage": false,
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/$1"
        },
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "node_modules",
            "main.ts",
            ".guard.ts",
            ".entity.ts",
            ".constants.ts",
            ".dto.ts",
            ".module.ts",
            ".middleware.ts",
            ".decorator.ts"
        ]
    }
}