    "apollo-server-express": "^3.8.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.30.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "fs-capacitor": "^8.0.0",
    "graphql": "^16.5.0",
    "graphql-upload": "13.0.0",
