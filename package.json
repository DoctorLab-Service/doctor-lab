{
  "name": "auth",
  "version": "0.0.1",
  "description": "",
  "author": "MaridiM Developments",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "npm": ">=9.0.0 <9.2.0",
    "node": ">=18.0.0 <19.3.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist && npm cache clean --force",
    "build": "npm install && npx @nestjs/cli build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest.js --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
    
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/common": "^9.0.9",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.9",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/platform-express": "^9.0.9",
    "@nestjs/typeorm": "^9.0.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/jest": "28.1.6",
    "apollo-server-express": "^3.10.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.30.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "fs-capacitor": "^8.0.0",
    "graphql": "^16.5.0",
    "graphql-upload": "13.0.0",
    "i18next": "^21.9.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "jest": "28.1.3",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "twilio": "^3.80.1",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.9",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "verbose": true,
    "collectCoverage": false,
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "main.ts",
      ".guard.ts",
      ".entity.ts",
      ".constants.ts",
      ".dto.ts",
      ".module.ts",
      ".middleware.ts",
      ".decorator.ts"
    ]
  }
}
